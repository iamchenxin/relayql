{
  "name": "relayql",
  "version": "0.1.0",
  "description": "the subset of GraphQL used by Relay",
  "main": "./lib/index.js",
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "npm run jest",
    "jest": "NODE_ENV=test jest $@",
    "lint": "node_modules/.bin/eslint .",
    "lint2": "node_modules/eslint/bin/eslint.js .",
    "bnode": "NODE_ENV=dev babel-node",
    "typecheck": "flow check",
    "build": "gulp clean && gulp build",
    "prepublish": "npm run build",
    "server": "npm run bnode -- example/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamchenxin/relayql.git"
  },
  "keywords": [
    "relay",
    "graphql",
    "react"
  ],
  "author": "iamchenxin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamchenxin/relayql/issues"
  },
  "homepage": "https://github.com/iamchenxin/relayql#readme",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-runtime": "^6.11.6",
    "flow-dynamic": "0.0.12",
    "flow-graphql": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^14.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-fbjs": "^2.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.2.2",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^2.6.1",
    "express": "^4.14.0",
    "fbjs-scripts": "^0.7.1",
    "flow-bin": "^0.32.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "jest-cli": "^14.1.0",
    "t-express-graphql": "^0.5.4"
  }
}
